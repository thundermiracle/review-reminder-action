{
  "name": "review-reminder-action",
  "version": "1.0.8",
  "author": "thundermiracle",
  "license": "MIT",
  "packageManager": "pnpm@8.15.9",
  "main": "lib/main.js",
  "scripts": {
    "format": "prettier --write '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "test": "pnpm lint && vitest run",
    "build": "tsup",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "pnpm build && pnpm format && pnpm test && pnpm package",
    "cs:version": "changeset version",
    "cs:tag": "changeset tag"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^5.1.1",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.9",
    "@swc/core": "^1.7.39",
    "@types/node": "^18.19.59",
    "@vercel/ncc": "^0.38.2",
    "@vitest/coverage-v8": "^0.32.2",
    "@web-configs/eslint-plugin": "^0.5.1",
    "@web-configs/prettier": "^0.1.2",
    "@web-configs/typescript": "^0.2.2",
    "eslint": "^8.57.0",
    "prettier": "^2.8.8",
    "tsup": "^7.2.0",
    "typescript": "^5.6.3",
    "vitest": "^0.32.2"
  },
  "prettier": "@web-configs/prettier",
  "tsup": {
    "entry": [
      "src/**/*.ts"
    ],
    "outDir": "lib",
    "splitting": true,
    "clean": true
  }
}
